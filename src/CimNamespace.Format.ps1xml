      <Name>WmiNamespace.table</Name>
      <ViewSelectedBy>
        <SelectionSetName>WmiNamespaces</SelectionSetName>
      </ViewSelectedBy>
      <GroupBy>
        <PropertyName>Namespace</PropertyName>
      </GroupBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
          </TableColumnHeader>
          <TableColumnHeader>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <EntrySelectedBy>
